</head>
<body ng-app="app">
    <!-- @NAV -->
    <!-- 通用头部 -->
<style>.m-botnav .mainnav .link{padding:0 13px;}</style>
    <div class="g-wrap">
        <div class="m-topnav rel">
            <div class="g-container clear">
